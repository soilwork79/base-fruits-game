{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/signScore",
      "dest": "/api/signScore.js"
    },
    {
      "src": "/api/leaderboard",
      "dest": "/api/leaderboard.js"
    },
    {
      "src": "/api/submitScore",
      "dest": "/api/submitScore.js"
    }
  ]
}
